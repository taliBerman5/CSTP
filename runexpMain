#!/usr/bin/env bash

DIR="unified_planning"               # Domain directory
LOG=/dev/stdout             # Log file output
REWIND=false
NUMRUN=10
SC_DEADLINE=10
MS_DEADLINE=27
NR_DEADLINE=20


